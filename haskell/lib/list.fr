(package list)

(type List
    (struct head tail)
)

(function is_empty [list]
    (is_null list)
)

(function tail [list]
    (member list tail)
)

(function head [list]
    (member list head)
)

(function map [func list]
    (if (is_empty list)
        list
        (List
            (func (head list))
            (map func (tail list))
        )
    )
)

(var my_list
    (List 4 (List 3 (List 2 (List 1 null))))
)

(function sq [x]
    (* x x)
)

(var main
    (map sq my_list)
)
